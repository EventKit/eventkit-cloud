{
  "name": "eventkit_cloud",
  "version": "1.13.0",
  "description": "Eventkit-cloud is based on the HOT OSM Export Tool 2. It allows the user to select data from different sources to export into a variety of formats.",
  "main": "webpack.config.js",
  "dependencies": {
    "@manaflair/redux-batch": "1.0.0",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "axios": "0.21.4",
    "connected-react-router": "6.8.0",
    "jsts": "2.8.1",
    "lodash": "4.17.21",
    "moment": "2.27.0",
    "normalizr": "3.6.0",
    "numeral": "2.0.6",
    "ol": "6.4.3",
    "prop-types": "15.7.2",
    "query-string": "6.13.1",
    "react": "16.13.1",
    "react-bootstrap-typeahead": "5.1.0",
    "react-custom-scrollbars": "4.2.1",
    "react-day-picker": "7.4.8",
    "react-dom": "16.13.1",
    "react-dropzone": "11.0.1",
    "react-joyride": "2.2.1",
    "react-loadable": "5.5.0",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-swipeable-views": "0.13.9",
    "react-swipeable-views-core": "0.13.7",
    "react-swipeable-views-utils": "0.13.9",
    "redux": "4.0.5",
    "redux-auth-wrapper": "3.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "react-virtuoso": "2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.17.10",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.16.8",
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.5",
    "@types/enzyme": "3.10.5",
    "@types/geojson": "7946.0.7",
    "@types/history": "4.7.2",
    "@types/jest": "26.0.4",
    "@types/material-ui": "0.21.7",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/redux-auth-wrapper": "2.0.9",
    "axios-mock-adapter": "1.18.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "9.2.0",
    "css-loader": "6.7.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "file-loader": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.1.0",
    "jest-canvas-mock": "2.2.0",
    "jest-cli": "26.1.0",
    "jest-diff": "26.6.2",
    "jsdom": "16.4.0",
    "mini-css-extract-plugin": "2.6.0",
    "msw": "0.21.0",
    "nightwatch": "1.3.6",
    "nock": "13.0.2",
    "node-polyfill-webpack-plugin": "1.1.4",
    "pixelmatch": "5.2.1",
    "pngjs": "5.0.0",
    "react-test-renderer": "16.13.1",
    "redux-logger": "3.0.6",
    "sinon": "9.0.2",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "26.1.1",
    "ts-loader": "9.3.0",
    "tslint": "6.1.2",
    "tslint-react": "5.0.0",
    "typescript": "3.9.6",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": true,
        "babelConfig": true
      }
    },
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/icons/**",
      "!**/config.js",
      "!**/*.config.{js, jsx}",
      "!**/setupFile.js",
      "!**/initialState.js",
      "!**/styles/**",
      "!**/node_modules/**",
      "!**/e2e/**",
      "!**/store/configureStore.js",
      "!**/store/configureTestStore.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/eventkit_cloud/ui/static/ui/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "staticfiles",
      "rootReducer.js",
      "index.tsx"
    ],
    "setupFiles": [
      "./setupFile.js",
      "jest-canvas-mock"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jsts|ol)/).+\\.js$"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/.(js|ts)?(x)",
      "**/?(*.)+(spec|test).(js|ts)?(x)"
    ]
  },
  "scripts": {
    "build": "PROD=1 node_modules/webpack/bin/webpack.js",
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "coverage": "node_modules/jest-cli/bin/jest.js --coverage --silent",
    "test": "node_modules/jest-cli/bin/jest.js --silent",
    "e2e": "node_modules/nightwatch/bin/nightwatch",
    "install-linter": "npm install eslint@4.9.0 eslint-plugin-import@2.7.0 eslint-plugin-react@7.4.0 eslint-plugin-jsx-a11y@6.0.2 eslint-config-airbnb@16.1.0",
    "eslint": "node_modules/eslint/bin/eslint.js './eventkit_cloud/ui/static/ui/app/**/*.js'",
    "tslint": "node_modules/tslint/bin/tslint './eventkit_cloud/ui/static/ui/app/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eventkit/eventkit-cloud.git"
  },
  "keywords": [
    "eventkit",
    "eventkit_cloud"
  ],
  "author": "jsvrcek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eventkit/eventkit-cloud/issues"
  },
  "homepage": "https://github.com/eventkit/eventkit-cloud#readme"
}
