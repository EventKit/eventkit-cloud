    "react-bootstrap-typeahead": "5.2.2",
    "react-custom-scrollbars": "4.2.1",
    "react-day-picker": "8.0.4",
    "react-dom": "17.0.2",
    "react-dropzone": "12.0.5",
    "react-joyride": "2.4.0",
    "react-loadable": "5.5.0",
    "react-redux": "8.0.1",
    "react-router": "5.3.1",
    "react-router-dom": "5.3.1",
    "react-swipeable-views": "0.14.0",
    "react-swipeable-views-core": "0.14.0",
    "react-swipeable-views-utils": "0.14.0",
    "redux": "4.2.0",
    "redux-auth-wrapper": "3.0.0",
    "redux-thunk": "2.4.1",
    "reselect": "4.1.5",
    "react-virtuoso": "2.9.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@babel/plugin-proposal-function-bind": "7.16.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-async-to-generator": "7.16.8",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-flow": "7.16.7",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@types/enzyme": "3.10.12",
    "@types/geojson": "7946.0.8",
    "@types/jest": "27.4.1",
    "@types/material-ui": "0.21.12",
    "@types/react": "18.0.6",
    "@types/react-dom": "18.0.2",
    "@types/react-redux": "7.1.24",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-auth-wrapper": "2.0.13",
    "axios-mock-adapter": "1.20.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.5",
    "cache-loader": "4.1.0",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "9.2.0",
    "css-loader": "6.7.1",
    "enzyme": "3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "eslint": "8.13.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "jest-canvas-mock": "2.3.1",
    "jest-cli": "27.5.1",
    "jest-diff": "27.5.1",
    "jest-environment-jsdom": "28.0.2",
    "jsdom": "19.0.0",
    "mini-css-extract-plugin": "2.6.0",
    "msw": "0.39.2",
    "nightwatch": "2.1.3",
    "node-polyfill-webpack-plugin": "1.1.4",
    "pixelmatch": "5.2.1",
    "pngjs": "6.0.0",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "18.0.0",
    "redux-logger": "3.0.6",
    "sinon": "13.0.2",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "write-file-webpack-plugin": "4.5.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {},
    "globals": {
      "ts-jest": {
        "tsconfig": true,
        "babelConfig": true
      }
    },
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/icons/**",
      "!**/config.js",
      "!**/*.config.{js, jsx}",
      "!**/setupFile.js",
      "!**/initialState.js",
      "!**/styles/**",
      "!**/node_modules/**",
      "!**/e2e/**",
      "!**/store/configureStore.js",
      "!**/store/configureTestStore.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/eventkit_cloud/ui/static/ui/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "staticfiles",
      "rootReducer.js",
      "index.tsx"
    ],
    "setupFiles": [
      "./setupFile.js",
      "jest-canvas-mock"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jsts|ol)/).+\\.js$"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).(js|ts)?(x)",
      "**/__tests__/**/.(js|ts)?(x)"
    ],
    "preset": "ts-jest/presets/js-with-babel"
  },
  "scripts": {
    "build": "PROD=1 node_modules/webpack/bin/webpack.js",
    "start": "node_modules/webpack/bin/webpack.js serve",
    "coverage": "node_modules/jest-cli/bin/jest.js --coverage --silent",
    "test": "node_modules/jest-cli/bin/jest.js --silent",
    "e2e": "node_modules/nightwatch/bin/nightwatch",
    "install-linter": "npm install eslint@4.9.0 eslint-plugin-import@2.7.0 eslint-plugin-react@7.4.0 eslint-plugin-jsx-a11y@6.0.2 eslint-config-airbnb@16.1.0",
    "eslint": "node_modules/eslint/bin/eslint.js './eventkit_cloud/ui/static/ui/app/**/*.js'",
    "tslint": "node_modules/tslint/bin/tslint './eventkit_cloud/ui/static/ui/app/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eventkit/eventkit-cloud.git"
  },
  "keywords": [
    "eventkit",
    "eventkit_cloud"
  ],
  "author": "jsvrcek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eventkit/eventkit-cloud/issues"
  },
  "homepage": "https://github.com/eventkit/eventkit-cloud#readme"
}
