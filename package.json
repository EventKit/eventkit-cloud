{
  "name": "eventkit_cloud",
  "version": "1.0.0",
  "description": "Eventkit-cloud is based on the HOT OSM Export Tool 2. It allows the user to select data from different sources to export into a variety of formats.",
  "main": "webpack.config.js",
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@manaflair/redux-batch": "0.1.0",
    "@material-ui/core": "3.3.2",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "1.0.0-alpha.12",
    "axios": "0.15.3",
    "jsts": "1.4.0",
    "lodash": "4.17.11",
    "moment": "2.20.1",
    "normalizr": "3.3.0",
    "numeral": "2.0.6",
    "ol": "4.4.2",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "react": "16.4.2",
    "react-bootstrap-typeahead": "2.4.0",
    "react-cookie": "1.0.5",
    "react-custom-scrollbars": "4.2.1",
    "react-day-picker": "7.2.4",
    "react-dom": "16.4.1",
    "react-dropzone": "4.2.13",
    "react-joyride": "1.11.4",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-swipeable-views": "0.12.18",
    "redux": "4.0.0",
    "redux-auth-wrapper": "2.0.3",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-function-bind": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-async-to-generator": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@types/enzyme": "3.1.14",
    "@types/geojson": "7946.0.4",
    "@types/jest": "23.3.2",
    "@types/material-ui": "0.21.5",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@types/react-joyride": "1.11.1",
    "@types/react-redux": "6.0.9",
    "@types/react-router": "3.0.5",
    "@types/react-router-redux": "4.0.51",
    "@types/redux-auth-wrapper": "2.0.8",
    "axios-mock-adapter": "1.7.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.4",
    "canvas": "1.6.13",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.12",
    "css-loader": "0.28.11",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "3.0.0",
    "jest-cli": "23.4.2",
    "jsdom": "9.12.0",
    "nightwatch": "0.9.16",
    "nock": "9.0.2",
    "pixelmatch": "4.0.2",
    "pngjs": "3.3.0",
    "react-addons-test-utils": "15.4.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "16.5.2",
    "sinon": "3.2.0",
    "style-loader": "0.21.0",
    "ts-jest": "23.1.4",
    "ts-loader": "5.2.2",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.1.4",
    "url-loader": "1.0.1",
    "webpack": "4.12.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "3.1.4",
    "write-file-webpack-plugin": "4.3.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": true,
        "babelConfig": true
      }
    },
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "**/app/**.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/icons/**",
      "!**/config.js",
      "!**/*.config.{js, jsx}",
      "!**/setupFile.js",
      "!**/initialState.js",
      "!**/styles/**",
      "!**/node_modules/**",
      "!**/e2e/**",
      "!**/store/configureStore.js",
      "!**/store/configureTestStore.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/eventkit_cloud/ui/static/ui/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "staticfiles",
      "rootReducer.js",
      "index.tsx"
    ],
    "setupFiles": [
      "./setupFile.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jsts|ol)/).+\\.js$"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/.(js|ts)?(x)",
      "**/?(*.)+(spec|test).(js|ts)?(x)"
    ]
  },
  "scripts": {
    "build": "PROD=1 node_modules/webpack/bin/webpack.js",
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline",
    "coverage": "node_modules/jest-cli/bin/jest.js --coverage --silent && mkdir -p ./coverage/coveralls && coveralls-lcov -v -n ./coverage/lcov.info > ./coverage/coveralls/coveralls.json",
    "test": "node_modules/jest-cli/bin/jest.js --silent",
    "e2e": "node_modules/nightwatch/bin/nightwatch",
    "install-linter": "npm install eslint@4.9.0 eslint-plugin-import@2.7.0 eslint-plugin-react@7.4.0 eslint-plugin-jsx-a11y@6.0.2 eslint-config-airbnb@16.1.0",
    "eslint": "node_modules/eslint/bin/eslint.js './eventkit_cloud/ui/static/ui/app/**/*.js'",
    "tslint": "node_modules/tslint/bin/tslint './eventkit_cloud/ui/static/ui/app/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eventkit/eventkit-cloud.git"
  },
  "keywords": [
    "eventkit",
    "eventkit_cloud"
  ],
  "author": "jsvrcek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eventkit/eventkit-cloud/issues"
  },
  "homepage": "https://github.com/eventkit/eventkit-cloud#readme"
}
