{
  "name": "eventkit_cloud",
  "version": "1.0.0",
  "description": "Eventkit-cloud is based on the HOT OSM Export Tool 2. It allows the user to select data from different sources to export into a variety of formats.",
  "main": "webpack.config.js",
  "dependencies": {
    "@babel/polyfill": "7.4.0",
    "@manaflair/redux-batch": "1.0.0",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "@material-ui/lab": "1.0.0-alpha.12",
    "axios": "0.19.0",
    "connected-react-router": "6.4.0",
    "jsts": "2.0.4",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "normalizr": "3.4.0",
    "numeral": "2.0.6",
    "ol": "4.6.5",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "raf": "3.4.1",
    "react": "16.8.6",
    "react-bootstrap-typeahead": "3.4.5",
    "react-custom-scrollbars": "4.2.1",
    "react-day-picker": "7.2.4",
    "react-dom": "16.8.6",
    "react-dropzone": "10.1.5",
    "react-joyride": "1.11.4",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-swipeable-views": "0.13.3",
    "redux": "4.0.1",
    "redux-auth-wrapper": "2.1.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/preset-env": "7.5.4",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@types/enzyme": "3.9.4",
    "@types/geojson": "7946.0.7",
    "@types/jest": "24.0.15",
    "@types/material-ui": "0.21.6",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-joyride": "1.11.1",
    "@types/react-redux": "7.1.0",
    "@types/react-router": "5.0.3",
    "@types/redux-auth-wrapper": "2.0.9",
    "@types/sinon": "^7.5.1",
    "axios-mock-adapter": "1.16.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.14.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "4.0.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "5.0.0",
    "jest": "24.8.0",
    "jest-canvas-mock": "2.1.0",
    "jest-cli": "24.8.0",
    "jsdom": "15.1.1",
    "mini-css-extract-plugin": "0.7.0",
    "nightwatch": "1.1.12",
    "nock": "10.0.6",
    "pixelmatch": "5.0.2",
    "pngjs": "3.4.0",
    "react-hot-loader": "4.11.1",
    "react-test-renderer": "16.8.6",
    "redux-logger": "3.0.6",
    "sinon": "7.3.2",
    "style-loader": "0.23.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "2.0.1",
    "webpack": "4.35.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "write-file-webpack-plugin": "4.5.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": true,
        "babelConfig": true
      }
    },
    "testURL": "http://localhost",
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "**/app/**.{js,jsx,ts,tsx}",
      "!**/*.d.ts",
      "!**/icons/**",
      "!**/config.js",
      "!**/*.config.{js, jsx}",
      "!**/setupFile.js",
      "!**/initialState.js",
      "!**/styles/**",
      "!**/node_modules/**",
      "!**/e2e/**",
      "!**/store/configureStore.js",
      "!**/store/configureTestStore.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/eventkit_cloud/ui/static/ui/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "modulePathIgnorePatterns": [
      "staticfiles",
      "rootReducer.js",
      "index.tsx"
    ],
    "setupFiles": [
      "./setupFile.js",
      "jest-canvas-mock"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(jsts|ol)/).+\\.js$"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/.(js|ts)?(x)",
      "**/?(*.)+(spec|test).(js|ts)?(x)"
    ]
  },
  "scripts": {
    "build": "PROD=1 node_modules/webpack/bin/webpack.js",
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline",
    "coverage": "node_modules/jest-cli/bin/jest.js --coverage --silent && mkdir -p ./coverage/coveralls && coveralls-lcov -v -n ./coverage/lcov.info > ./coverage/coveralls/coveralls.json",
    "test": "node_modules/jest-cli/bin/jest.js --silent",
    "e2e": "node_modules/nightwatch/bin/nightwatch",
    "install-linter": "npm install eslint@4.9.0 eslint-plugin-import@2.7.0 eslint-plugin-react@7.4.0 eslint-plugin-jsx-a11y@6.0.2 eslint-config-airbnb@16.1.0",
    "eslint": "node_modules/eslint/bin/eslint.js './eventkit_cloud/ui/static/ui/app/**/*.js'",
    "tslint": "node_modules/tslint/bin/tslint './eventkit_cloud/ui/static/ui/app/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eventkit/eventkit-cloud.git"
  },
  "keywords": [
    "eventkit",
    "eventkit_cloud"
  ],
  "author": "jsvrcek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eventkit/eventkit-cloud/issues"
  },
  "homepage": "https://github.com/eventkit/eventkit-cloud#readme"
}
