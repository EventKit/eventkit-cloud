    "axios": "0.26.1",
    "connected-react-router": "6.9.2",
    "date-fns": "2.28.0",
    "jsts": "2.8.1",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "normalizr": "3.6.2",
    "numeral": "2.0.6",
    "ol": "6.5.0",
    "prop-types": "15.8.1",
    "query-string": "7.1.1",
